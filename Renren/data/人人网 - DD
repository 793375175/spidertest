function cutImg(img){
if(img.width >= img.height) {
img.width = 30 * img.width / img.height;
img.height = 30;
offset = (img.width - 30) * 0.5;
img.style.left = -offset + 'px';
} else {
img.height = 30 * img.height / img.width;
img.width = 30;
offset = (img.height - 30) * 0.5;
img.style.top = -offset + 'px';
}
}
</script>
